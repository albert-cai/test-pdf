  "babel": {
    "presets": [
      "react",
      "node5",
      "stage-0"
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire",
          "transform-object-rest-spread",
          "transform-es2015-destructuring"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
      "__DEV__": true
    },
    "env": {
      "browser": true
    },
    "rules": {
      "no-confusing-arrow": 0,
      "react/jsx-quotes": 0,
      "jsx-quotes": [
        2,
        "prefer-double"
      ]
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global",
            "local"
          ]
        }
      ]
    }
  },
  "scripts": {    
    "eslint": "eslint src tools",
    "stylelint": "stylelint \"src/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "cross-env NODE_ENV=test nyc --extension .jsx --extension .es6 --reporter=lcov --reporter=text mocha \"test/**/*@(.js|.jsx)\" --require test/test_helper.js --compilers js:babel-core/register", 
    "testwithoutreport": "mocha \"test/**/*@(.js|.jsx)\" --require test/test_helper.js --compilers js:babel-core/register", 
    "test:watch": "npm run test -- --reporter min --watch",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "NODE_ENV=production babel-node tools/run build",
    "build:qa": "NODE_ENV=qa babel-node tools/run build",
    "build:qa2": "NODE_ENV=qa2 babel-node tools/run build",
    "build:uat": "NODE_ENV=uat babel-node tools/run build",
    "build:uat2": "NODE_ENV=uat2 babel-node tools/run build",
    "build:production": "NODE_ENV=production babel-node tools/run build",
    "deploy": "babel-node tools/run deploy",
    "push": "NODE_ENV=qa babel-node tools/run push",
    "push:qa2": "NODE_ENV=qa2 babel-node tools/run push",
    "push:uat": "NODE_ENV=uat babel-node tools/run push",
    "push:uat2": "NODE_ENV=uat2 babel-node tools/run push",
    "push:production": "NODE_ENV=production babel-node tools/run push",
    "render": "babel-node tools/run render",
    "start": "babel-node tools/run start",
    "updateDeps": "npm update --save | npm update --save-dev | webpack"
  }
